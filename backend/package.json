    },
    "devDependencies": {
        "@types/lodash": "4.14.110",
        "@types/mocha": "5.2.3",
        "@types/chai": "4.1.4",
        "@types/pg": "7.4.10",
        "@types/es6-promise": "3.3.0",
        "@types/express": "4.16.0",
        "@types/node": "10.5.2",
        "@types/cors": "2.8.4",
        "typescript": "2.9.2",
        "ts-loader": "4.4.1",
        "ts-node": "7.0.0",
        "tslint": "5.10.0",
        "tslint-loader": "3.6.0",
        "cross-env": "^5.0.5",
        "webpack": "4.11.1",
        "webpack-cli": "3.0.3",
        "webpack-node-externals": "1.7.2",
        "nodemon": "1.17.5",
        "nodemon-webpack-plugin": "next",
        "mocha": "5.2.0",
        "chai": "4.1.2"
    }
